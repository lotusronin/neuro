extern fn putchar(c : int) : int;
extern fn puts(s :*u8) : int;

fn main() : u32 {
    i := 0;
    while(i < 10) {
        printi(i);
        putchar('\n');
        i = i+1;
    }
    return 0;
}

fn printi(i : u32) : u32 {
    if(i < 0) {
        putchar('-');
        i = 0-i;
    }
    if(i < 10) {
        putchar('0'+i);
    } else {
        c := i/10;
        printi(c);
        n := i - c*10;
        putchar('0'+n);
    }
    return 0;
}
