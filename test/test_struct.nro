import stdio;

struct point {
    x : u32;
    y : u32;
    z : u32;
}

fn main() : u32 {
    foo : point;
    foo.x = 0;
    foo.y = 1;
    foo.z = 2;
    stdio::puts("point at (x,y,z)");
    stdio::printi(foo.x);
    stdio::putchar(',');
    stdio::printi(foo.y);
    stdio::putchar(',');
    stdio::printi(foo.z);
    stdio::putchar('\n');
    return 0;
}
